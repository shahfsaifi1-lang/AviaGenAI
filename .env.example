# Gemini
GOOGLE_API_KEY=<your-google-ai-studio-key>
GEMINI_MODEL=gemini-1.5-flash
REQUEST_MAX_OUTPUT_TOKENS=600
REQUEST_TEMPERATURE=0.2

# MetService NZ (set when you have access)
METSERVICE_BASE_URL=
METSERVICE_API_KEY=
METSERVICE_METAR_PATH=/aviation/metar
METSERVICE_TAF_PATH=/aviation/taf
# Optional one-minute weather endpoint if you gain access
METSERVICE_MINUTE_PATH=

# MET Norway fallback (public)
METNO_USER_AGENT=AviaGenAI/0.1 (contact: you@example.com)

# Decision engine defaults (tune to your authority/corrosponding AIP)
VFR_MIN_VIS_KM=5
VFR_MIN_CEILING_FT=3000
MVFR_MIN_VIS_KM=5
MVFR_MIN_CEILING_FT=1000
T6_MAX_XWIND_KT= # set if you have an approved training-aid value, else leave empty
